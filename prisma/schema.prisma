generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model guest {
  idx     Int     @id @default(autoincrement())
  content String? @db.VarChar(600)
  name    String? @db.VarChar(50)
}

model photo {
  img_id     String @id @db.VarChar(600)
  like_count Int    @default(0)
}
